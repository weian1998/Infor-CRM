          <Action typeName="Sage.Platform.QuickForms.ActionItems.CSharpSnippetActionItem, Sage.Platform.QuickForms">
            <CSharpSnippetActionItem>
              <MethodName>btnManageSchema_ClickAction</MethodName>
              <CSharpCodeSnippet>if (DialogService != null)
{
    Sage.Entity.Interfaces.IIntegration integration = BindingSource.Current as Sage.Entity.Interfaces.IIntegration;
    //make sure there is at least one defined integration link
    if (integration.IntegrationLinks.Count == 0)
    {
        throw new Sage.Platform.Application.ValidationException(GetLocalResourceObject("error_UndefinedLink").ToString());
    }
    // DialogActionItem
    DialogService.SetSpecs(400, 600, "AddEditAccountingSystem", GetLocalResourceObject("931ca039-d6f1-428d-aac0-92b2b6d9c8e3.DialogTitleOverride").ToString());
    DialogService.EntityType = typeof (Sage.Entity.Interfaces.IAppIdMapping);
    DialogService.EntityID = integration.IntegrationLinks.FirstOrDefault().Id.ToString();
    DialogService.ShowDialog();
}</CSharpCodeSnippet>
            </CSharpSnippetActionItem>
          </Action>
